  <script type="text/javascript">
	function check()
	{
		 var key;
		 key=$("#keys").val().replace(/(^\s*)|(\s*$)/g, "");
		 if(key==''||key=='输入您想搜索的内容'){
			art.dialog({content:'关键词不能为空',time:2});
			return false;
		 }
		 else
		 {
			  return true;
		 }
	}
